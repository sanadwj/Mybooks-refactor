{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "arrow-parens": "off",
    "import/extensions": "off",
    "arrow-body-style":"off",
    "func-names":"off",
    "object-curly-newline":"off"
  },
  "ignorePatterns": [
    "dist/",
    "build/"
  ]
}